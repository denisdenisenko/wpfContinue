<Window x:Class="wpfContinue.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfContinue"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="File">
                <MenuItem Header="New">
                    <MenuItem Header="Project" Click="MenuItem_Click">
                        <MenuItem.Icon>
                            <Image Source="https://upload.wikimedia.org/wikipedia/commons/thumb/7/73/Flat_tick_icon.svg/1200px-Flat_tick_icon.svg.png"></Image>
                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="File"></MenuItem>
                </MenuItem>
                <MenuItem Header="Open"></MenuItem>
                <MenuItem Header="Clone Repository"></MenuItem>


            </MenuItem>
            
        </Menu>
        
    </DockPanel>
</Window>
